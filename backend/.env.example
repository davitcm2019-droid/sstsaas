NODE_ENV=development
PORT=5000

# Obrigat├│rias
# A API n├úo inicia sem elas.
JWT_SECRET=
CORS_ORIGIN=http://localhost:5173

# Opcionais
JWT_EXPIRES_IN=24h
BCRYPT_SALT_ROUNDS=10

# (Opcional) Senhas para usu├írios mock (backend/src/data/mockData.js)
# DEMO_ADMIN_PASSWORD=
# DEMO_TECH_1_PASSWORD=
# DEMO_TECH_2_PASSWORD=
# DEMO_VIEWER_PASSWORD=

# Consulta de CNPJ (CNPJA Open)
CNPJA_API_URL=https://api.cnpja.com/office
CNPJA_TIMEOUT_MS=8000
# Cache de consultas (segundos) - recomendado 12-24h
CNPJA_CACHE_TTL_SECONDS=86400
# User-Agent enviado para a API
CNPJA_USER_AGENT=SST-SaaS/1.0
# Se a CNPJA exigir autenticação, configure sua chave abaixo:
# CNPJA_API_KEY=
# CNPJA_API_KEY_HEADER=Authorization
# Prefixo opcional (ex.: Bearer). Por padrão, nenhum prefixo é usado.
# CNPJA_API_KEY_PREFIX=
