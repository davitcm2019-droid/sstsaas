NODE_ENV=development
PORT=5000

# Obrigatórias (a API não inicia sem elas)
JWT_SECRET=
CORS_ORIGIN=http://localhost:3000

# Postgres (opcional): quando configurado, usuários/auth usam o banco e migrations rodam no start
# DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DATABASE
# DATABASE_SSL=false

# Opcionais
JWT_EXPIRES_IN=24h
BCRYPT_SALT_ROUNDS=10

# (Opcional) Senhas para usuários demo (aplicadas no banco ao iniciar, se DATABASE_URL estiver setado)
# DEMO_ADMIN_PASSWORD=
# DEMO_TECH_1_PASSWORD=
# DEMO_TECH_2_PASSWORD=
# DEMO_VIEWER_PASSWORD=
